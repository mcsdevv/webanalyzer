{
    "version": 2,
    "builds": [
      { "src": "api/analyze.ts", "use": "@vercel/node" },
      { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
    ],
    "routes": [
      { "src": "/api/analyze", "methods": ["POST", "OPTIONS"], "dest": "/api/analyze.ts" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }