{
  "version": 2,
  "builds": [
    { "src": "dist/api/**/*.js", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/dist/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}